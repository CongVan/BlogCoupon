    <script>

        $(document).ready(function () {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 400) {
                    $('#ScrollToTop').fadeIn();

                } else {
                    $('#ScrollToTop').fadeOut();

                }
            });
            // scroll body to 0px on click
            $('#ScrollToTop').click(function () {
                $('#ScrollToTop').tooltip('hide');
                $('body,html').animate({
                    scrollTop: 0
                }, 400);
                return false;
            });
        });
